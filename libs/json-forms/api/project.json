{
  "name": "json-forms-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/json-forms/api/src",
  "projectType": "library",
  "release": {
    "version": {
      "currentVersionResolver": "git-tag",
      "preserveLocalDependencyProtocols": false,
      "manifestRootsToUpdate": [
        "{projectRoot}",
        "dist/{projectRoot}"
      ]
    }
  },
  "tags": [
    "scope:feature",
    "scope:api"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/json-forms/api",
        "tsConfig": "libs/json-forms/api/tsconfig.lib.json",
        "packageJson": "libs/json-forms/api/package.json",
        "main": "libs/json-forms/api/src/index.ts",
        "assets": [
          "libs/json-forms/api/*.md"
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  },
  "implicitDependencies": [
    "json-forms-core"
  ]
}
